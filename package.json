{
  "name": "railrouter-sg",
  "version": "1.0.0",
  "description": "Explore MRT and LRT rail routes in Singapore",
  "keywords": [
    "singapore",
    "mrt",
    "lrt",
    "train",
    "metro",
    "subway",
    "map"
  ],
  "author": "Lim Chee Aun <cheeaun@gmail.com> (http://cheeaun.com/)",
  "license": "MIT",
  "scripts": {
    "serve": "serve",
    "citymapper-routes": "node scripts/fetch-citymapper-routes",
    "citymapper-stops": "node scripts/fetch-citymapper-stops",
    "citymapper-all": "node scripts/compile-citymapper",
    "wikipedia": "node scripts/fetch-wikipedia",
    "all": "npm run citymapper-routes && npm run citymapper-stops && npm run wikipedia && npm run citymapper-all",
    "_lines": "node scripts/fetch-lines.js",
    "_lines-all": "node scripts/from-lines-compile-all.js",
    "_exits": "node scripts/fetch-exits-citymapper.js",
    "_exits-all": "node scripts/fetch-onemap-exits-recompile-all.js",
    "_wikipedia-all": "node scripts/fetch-wikipedia-recompile-all.js",
    "_smrt": "node scripts/fetch-smrt.js",
    "inliner": "node scripts/inliner.js _index.html index.html",
    "watch": "chokidar '_index.html' 'assets/*.js' 'assets/*.css' 'data/v2/all.json' -c 'npm run inliner' -d 1000",
    "start": "run-p watch serve",
    "deploy": "git checkout gh-pages && git merge master && git push origin gh-pages && git checkout master"
  },
  "devDependencies": {
    "cheerio": "~0.22.0",
    "chokidar-cli": "~1.2.0",
    "clean-css": "~4.1.11",
    "got": "~8.3.1",
    "json5": "~1.0.1",
    "npm-run-all": "^4.1.3",
    "polyline": "~0.2.0",
    "serve": "^9.0.0",
    "uglify-js": "^3.4.0",
    "xml2js": "~0.4.19"
  }
}
