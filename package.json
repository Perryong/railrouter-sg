{
  "name": "railrouter-sg",
  "version": "2.0.0",
  "description": "Explore MRT and LRT rail routes in Singapore",
  "keywords": [
    "singapore",
    "mrt",
    "lrt",
    "train",
    "metro",
    "subway",
    "map",
    "transport"
  ],
  "author": "Lim Chee Aun <cheeaun@gmail.com> (http://cheeaun.com/)",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve ./src/index.html --no-autoinstall",
    "start-https": "parcel serve ./src/index.html --no-autoinstall --https",
    "build": "parcel build ./src/index.html",
    "postbuild": "cpy public/* dist"
  },
  "dependencies": {
    "fuse.js": "~6.4.0",
    "mapbox-gl": "~1.11.1"
  },
  "devDependencies": {
    "cpy-cli": "~3.1.1",
    "parcel": "~2.0.0-beta.1"
  },
  "browserslist": "> 0.25%, not ie 11, not op_mini all"
}
